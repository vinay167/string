#include<iostream>
using namespace std;
int main()
{
    int length,p,l,length1;
    int v=0,k=0;
    string s1,s2;
    cout<<"Enter the first string: ";
    cin>>s1;
    cout<<"Enter the second string: ";
    cin>>s2;
    length1=s2.length();
    length=s1.length();
    for(int i=0;i<length;i++)
    {
        p=(int)s1[i];
        v=p-96;
        cout<<v<<",";
    }
    cout<<""<<endl;
    for(int i=0;i<length1;i++)
    {
        l=(int)s2[i];
        k=l-96;
        cout<<k<<",";
    }
}
